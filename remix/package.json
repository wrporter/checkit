{
  "name": "remix-b136",
  "private": true,
  "description": "",
  "license": "",
  "sideEffects": false,
  "scripts": {
    "postinstall": "remix setup node",
    "build": "run-s build:*",
    "build:css": "npm run generate:css -- --minify",
    "build:remix": "remix build",
    "dev": "run-p dev:*",
    "dev:css": "npm run generate:css -- --watch",
    "dev:remix": "cross-env NODE_ENV=development binode --require ./mocks -- @remix-run/dev:remix dev",
    "format": "prettier --write .",
    "generate:css": "tailwindcss -o ./app/styles/tailwind.css",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "lint:fix": "npm run lint -- --fix",
    "start": "remix-serve build",
    "start:mocks": "binode --require ./mocks -- @remix-run/serve:remix-serve build",
    "test": "vitest",
    "typecheck": "tsc -b",
    "validate": "run-p \"test -- --run\" lint typecheck"
  },
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build"
  ],
  "dependencies": {
    "@heroicons/react": "^1.0.6",
    "@reach/checkbox": "^0.16.0",
    "@reach/dialog": "^0.16.2",
    "@reach/menu-button": "^0.16.2",
    "@reach/tooltip": "^0.16.2",
    "@reach/utils": "^0.16.0",
    "@remix-run/node": "^1.3.5",
    "@remix-run/react": "^1.3.5",
    "@remix-run/serve": "^1.3.5",
    "@remix-run/server-runtime": "^1.3.5",
    "bcryptjs": "^2.4.3",
    "classnames": "^2.3.1",
    "mongodb": "^4.5.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix-auth": "^3.2.1",
    "remix-auth-form": "^1.1.1",
    "remix-auth-google": "^1.1.0",
    "tiny-invariant": "^1.2.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^6.1.1",
    "@remix-run/dev": "^1.3.5",
    "@remix-run/eslint-config": "^1.3.5",
    "@testing-library/cypress": "^8.0.2",
    "@testing-library/dom": "^8.12.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^12.1.4",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^14.0.4",
    "@types/bcryptjs": "^2.4.2",
    "@types/eslint": "^8.4.1",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.23",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "@vitejs/plugin-react": "^1.3.0",
    "autoprefixer": "^10.4.4",
    "binode": "^1.0.5",
    "c8": "^7.11.0",
    "cross-env": "^7.0.3",
    "cypress": "^9.5.3",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-compat": "^4.0.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.1.4",
    "eslint-plugin-jest-dom": "^4.0.1",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.4.0",
    "eslint-plugin-testing-library": "^5.2.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "happy-dom": "^2.55.0",
    "msw": "^0.39.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.12",
    "prettier": "^2.6.1",
    "prettier-plugin-tailwindcss": "^0.1.8",
    "start-server-and-test": "^1.14.0",
    "tailwindcss": "^3.0.23",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^3.14.1",
    "typescript": "^4.6.3",
    "vite": "^2.9.1",
    "vite-tsconfig-paths": "^3.4.1",
    "vitest": "^0.9.2"
  },
  "engines": {
    "node": ">=14"
  },
  "jest": {
    "clearMocks": true,
    "resetMocks": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!**/*.test.util.ts",
      "!src/index.tsx",
      "!src/**/*.d.ts",
      "!src/setupProxy.js",
      "!src/setupTests.ts"
    ],
    "coverageThreshold": {
      "global": {
        "lines": 25,
        "branches": 25,
        "functions": 25,
        "statements": 25
      }
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "not ie 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
